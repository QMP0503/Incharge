@model Incharge.ViewModels.GymClassVM;

@{
    ViewData["Title"] = "Add Gym Class";
}

<div class="container">
    <div class="card">
        <div class="card-header">
            <h1>Add New Class</h1>
        </div>

        <div class="card-body">
            <form asp-action="AddGymclass" method="post" enctype="multipart/form-data">
                <!--Location Form fields partial view-->
                <partial name="_ValidationScriptsPartial" />
                <partial name="_GymClassFormFields" model="@Model" />
            </form>

        </div>

    </div>
</div>

